pipeline {
    agent any
    tools {
          maven 'MAVEN_HOME'
          jdk 'JAVA_HOME'
        }

    stages {
        stage('clean'){
            steps{
            sh """rm -rf *"""
            }}
        stage('Build') {
            steps {
                git branch: 'amine_branch',
                credentialsId: 'git_pwd_pat',
                url: 'https://github.com/AS-Byte/team1_devops_project.git'
            }
        }
        stage('MVN-CLEAN') {
                            steps {
                                sh """mvn clean"""
                            }
                        }
       stage('MVN-COMPILE') {
                  steps {
                                 sh """mvn compile"""
                        }
                             }
        stage('MVN-SONARQUBE') {
                    steps {
                    withSonarQubeEnv('SonarQube')
                    {
                     sh """mvn sonar:sonar"""
                                                  }
                           }
                                }
    }
}