pipeline {
    agent any
    tools {
    maven "M2_HOME"
    }
    stages {
        stage('Build') {
            steps {
                git branch: 'amine_branch', url: 'https://github.com/AS-Byte/team1_devops_project.git',
                credentialsId: 'git_pwd_pat'
                   }
                        }
        stage('Mvn-VERSION') {
            steps {
                sh """mvn -version"""
                  }
                        }
        stage('Mvn-ClEAN') {
            steps {
                 sh """mvn -X clean"""
                    }
                            }
        stage('Mvn-COMPILE') {
            steps {
                  sh """mvn compile"""
                   }
                              }
        stage('Mvn-SONARQUBE') {
            steps {
            withSonarQubeEnv('SonarQube')
                {sh """mvn sonar:sonar"""}
                   }
                            }
    }
   }