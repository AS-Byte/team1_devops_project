pipeline {
    agent any
   
    stages {
        stage('hello') {
            steps {
                echo "hello world";
               
             }
         }
         stage('MVN-CLEAN') {
                            steps {
                                sh """mvn clean"""
                            }
                        }

       stage('MVN-COMPILE') {
                  steps {
                                 sh """mvn compile"""
                                       }
                                       }

        stage('mvn-SONARQUBE') {
                    steps {
                    withSonarQubeEnv('sonarQube'){
                     sh """mvn sonar:sonar"""
                    }

                    }
    }
}
